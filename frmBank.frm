Private Sub cmdSubmit_Click()
    On Error GoTo errormsg
    Dim opttype As String
    If txtPurpose.Text <> "" And txtAmount.Text <> "" Then
        If optDebit.Value = True Then
            opttype = "Debit"
        End If
        If optCredit.Value = True Then
            opttype = "Credit"
        End If
        
        reccheck
        rec.Open "insert into tblBank values('" & DTPicker1.Value & "','" & txtPurpose.Text & "','" & txtAmount.Text & "','" & opttype & "')", con, adOpenDynamic, adLockOptimistic
        MsgBox "Data inserted successfully", vbInformation
        txtPurpose.Text = ""
        txtAmount.Text = ""
    Else
        MsgBox "All fields are mandatory", vbCritical
    End If
    Exit Sub
errormsg:
    MsgBox "Oops!!! Something went wrong", vbCritical
End Sub

